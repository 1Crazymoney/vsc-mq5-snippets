    "pipSizeMT5": {
        "body": 
        [
            "double point = SymbolInfoDouble(${1:_Symbol}, SYMBOL_POINT);",
            "int digit = (int)SymbolInfoInteger(${1:_Symbol}, SYMBOL_DIGITS);",
            "int mult = digit == 3 || digit == 5 ? 10 : 1;",
            "double pipSize = point * mult;"
        ],
        "description": "MT5: Get pip size",
        "prefix": "pipSizeMT5",
        "scope": "source.mq4"
    },
